<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.otherDao.SysStateMapper">
    <select id="selectSysStateByName" resultType="pojo.other.SysState">
        select * from css.sys_state
        where departmentName=#{departmentName} and opCourseType=#{opCourseType}
    </select>
</mapper>